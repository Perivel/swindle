{
  "name": "@swindle/cqrs",
  "version": "2.2.3",
  "description": "An implementation of the CQRS pattern.",
  "keywords": [
    "swindle",
    "cqrs"
  ],
  "author": "PatrickLumenus <patrickaluy@gmail.com>",
  "homepage": "https://github.com/Perivel/swindle.js#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/index.js",
        "require": "./dist/index.js"
      },
      "node": {
        "import": "./dist/server.js",
        "require": "./dist/server.js"
      },
      "import": "./dist/index.js",
      "require": "./dist/server.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perivel/swindle.js.git"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "inspect:circular": "npx madge --circular --extensions ts ./",
    "clean": "rimraf ./dist",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'"
  },
  "bugs": {
    "url": "https://github.com/Perivel/swindle.js/issues"
  },
  "dependencies": {
    "@swindle/core": "^2.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2"
  }
}
